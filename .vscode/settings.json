{
  "files.exclude": {
    "**/build": true,
    "**/ext": true,
    "**/__pycache__": true,
    "**/*.egg-info": true,
    "**/*~": true,
    "**/*.log": true,
    "**/node_modules": true,
    "**/.sencha": true,
    "**/example": true,
    "**/bootstrap*": true,
    "**/resources": true
